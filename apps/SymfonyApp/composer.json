{
  "name": "tadeskione/payment-processor-app-symfony",
  "description": "The Symfony-based app for PaymentProcessor",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/yaml": "^6.4",
    "guzzlehttp/guzzle": "^7.2",
    "tadeskione/payment-processor": "*"
  },
  "autoload": {
    "psr-4": {
      "PaymentProcessor\\Apps\\Symfony\\": "src"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../src",
      "options": {
        "symlink": true
      }
    }
  ],
  "minimum-stability": "stable",
  "version": "1.0.0"
}